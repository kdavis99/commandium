{
   "name": "Chrome Commands TODO(kylee): better name",
   "description": "TODO(kylee): be clever about this later.",
   "version": "1.0",
   "permissions": [
      "tabs", "browsingData", "http://*/*", "https://*/*"
   ],
   "browser_action": {
      "default_title": "Set this page's color.",
      "default_icon": "icon.png",
      "default_popup": "popup.html"
   },
   "commands": {
      "toggle-feature-submit": {
         "suggested_key": {
            "default": "Ctrl+Shift+K",
            "mac": "Command+Shift+K"
         },
         "description": "Click submit for user"
      },
      "_execute_browser_action": {
         "suggested_key": {
            "windows": "Ctrl+Shift+Y",
            "mac": "Command+Shift+Space",
            "chromeos": "Ctrl+Shift+U",
            "linux": "Ctrl+Shift+J"
         }
      }
   },
   "manifest_version": 2
}
